{
    "name": "pcnnet/flarum-auth-discord",
    "description": "Allow users to log in with Discord.",
    "type": "flarum-extension",
    "keywords": ["authentication"],
    "license": "MIT",
    "authors": [
        {
            "name": "Parsonswy",
            "email": "parsonswy@gmail.com"
        }
    ],
    "require": {
        "flarum/core": "^0.1.0-beta.8.1",
        "wohali/oauth2-discord-new": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "pcnnet\\discord\\": "src/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "0.1.x-dev"
        },
        "flarum-extension": {
            "title": "Discord Login",
            "icon": {
                "name": "fab fa-discord",
                "backgroundColor": "#7289da",
                "color": "#FFF"
            }
        }
    }
}